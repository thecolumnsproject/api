<body>
	
	<section class="share-page-header">
		<i class="clmnz-icon-columns"></i>
		The Columns Project
	</section>
	{{#if embed_id}}
		<section class="share-page-embed">
			<script type="text/javascript"
				src="{{ embed_host }}/public/embed-table.js"
				data-table-id="{{ embed_id }}"
				data-hosted-page="true">
			</script>
		</section>
		<section class="share-page-actions">
				<div class="share-page-actions-social">
					<a class="columns-button button-tertiary share-page-action-button tweet" href="https://twitter.com/intent/tweet?text={{tweet_body}}&url={{embed_uri}}" target="_blank">
						<i class="clmnz-icon-twitter"></i>
						Tweet this table
					</a>
					<a class="columns-button button-tertiary share-page-action-button email" href="mailto:?subject={{email_subject}}&body={{email_body}}" target="_blank">
						<i class="clmnz-icon-email"></i>
						Email this table
					</a>
				</div>

			<div class="share-page-action-create">
				<div class="share-page-create-label">Want to share your own data?</div>

				{{!-- <div class="share-page-create">
					{{embed_host}}/{{embed_id}}
				</span> --}}
				<a class="share-page-create columns-button button-secondary button-small" href="{{home_host}}" target="_blank">
						Create your own table
					</a>
			</div>

			<div class="share-page-action-link">
				<a href="mailto:jeremy@thecolumnsproject.com?subject=Feedback on The Columns Project">Have feedback?</a>
			</div>
		</section>
	{{else}}
		<section class="share-page-error">

			<div class="share-page-error-message">
				<div class="share-page-error-message-header">
					Shucks, we couldn't find this table.
				</div>
				<div class="share-page-error-message-subheader">
					Whatd'ya say we table this for now?<br />
					Feedback on both technical problems<br />
					and terrible puns is highly encouraged.
				</div>
			</div>

			<div class="share-page-action-link">
				<a href="mailto:jeremy@thecolumnsproject.com?subject=Feedback on The Columns Project">Have feedback?</a>
			</div>
		</section>
	{{/if}}

	<!-- Application Javascript -->
	<script type="text/javascript" src="js/share-page.js"></script>
</body>